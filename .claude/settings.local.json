{
  "permissions": {
    "allow": [
      "Bash(dotnet ef database update:*)",
      "Bash(cat:*)",
      "Bash(findstr:*)",
      "Bash(taskkill:*)",
      "Bash(dotnet run:*)",
      "Bash(dotnet build:*)",
      "Bash(npm:*)",
      "Bash(python:*)",
      "Bash(pytest:*)",
      "Bash(ls:*)",
      "Bash(dir:*)",
      "Bash(pwd:*)",
      "Bash(cd:*)",
      "Bash(mkdir:*)",
      "Bash(cp:*)",
      "Bash(mv:*)",
      "Bash(tee:*)",
      "Bash(code --install-extension github.codespaces)",
      "Bash(code --install-extension:*)",
      "Bash($null)",
      "Bash(powershell.exe -ExecutionPolicy Bypass -File .temp_ia/fix-all-conflicts.ps1)"
    ],
    "deny": [],
    "ask": [],
    "defaultMode": "bypassPermissions"
  },
  "outputStyle": "default",
  "sandbox": {
    "autoAllowBashIfSandboxed": true,
    "allowUnsandboxedCommands": true
  }
}
